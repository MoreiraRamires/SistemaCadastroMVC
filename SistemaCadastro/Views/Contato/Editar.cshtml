@{
    ViewData["Title"] = "Editar ";
}

<div class="text-left">
    <h1 class="display-4">Editando Contato</h1>

  

    <form>
        <div class="form-row">
            <div class="form-group col-md-12">
                <label for="inputNome">Nome</label>
                <input type="text" class="form-control" id="inputNome" placeholder="Rafael">
            </div>
            <div class="form-group col-md-6">
                <label for="inputEmail">Email</label>
                <input type="email" class="form-control" id="inputEmail" placeholder="rafael@gmail.com">
            </div>


            <div class="form-group col-md-6">
            <label for="inputCelular">Celular</label>
            <input type="text" class="form-control" id="inputCelular" placeholder="1234">
        </div>
        </div>
        

    </form>
    
</div>
